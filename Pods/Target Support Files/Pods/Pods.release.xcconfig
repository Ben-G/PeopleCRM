GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ReactiveCocoa" "${PODS_ROOT}/Headers/Public/STTwitter"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ReactiveCocoa" -isystem "${PODS_ROOT}/Headers/Public/STTwitter"
OTHER_LDFLAGS = -ObjC -l"Pods-ReactiveCocoa" -l"Pods-STTwitter" -framework "Accounts" -framework "CoreGraphics" -framework "Foundation" -framework "QuartzCore" -framework "Twitter" -framework "UIKit" -weak_framework "Social"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods